<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="hr_recruitment_form">
        <field name="name">hr.recruitment.show.questions</field>
        <field name="model">hr.recruitment.stage</field>
        <field name="inherit_id" ref="hr_recruitment.hr_recruitment_stage_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='job_ids']" position="after">

                <field name="interview_stages"/>

                    </xpath>


        </field>
    </record>




</odoo>